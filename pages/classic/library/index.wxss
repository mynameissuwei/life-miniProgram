.container .header {
  display: flex;
  width:100%;
  position:fixed;
  height: 170rpx;
  justify-content: center;
  z-index:999;
}
.container .header input {
  width: 80%;
  height: 80rpx;
  border-radius: 50px;
  border: 1px solid black;
  background-color:white;
  margin-top: 40rpx;
  text-align: center;
  background-color: #f5f5f5;
}

.container .header image {
  width:30rpx;
  height:30rpx;
  position: relative;
  left: 160rpx;
  bottom: 59rpx;
  z-index: 100;
  padding-top:100rpx;
}
.quality {
  margin-left: 300rpx;
  width: 100rpx;
  height: 40rpx;
  margin-top:140rpx;
}
.library {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
  height: 100%;
  width: 100%;
  background-color: #f5f5f5;
  padding-top:10rpx;
}
